<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<HBox fx:controller="hashtools.gui.window.application.ApplicationController" fx:id="paneRoot" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="480.0" prefWidth="853.0" styleClass="pane-root" stylesheets="@Application.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <VBox HBox.hgrow="NEVER" alignment="CENTER" fx:id="leftPane" minWidth="-Infinity" styleClass="left-pane">
        <Button alignment="CENTER_RIGHT" fx:id="buttonChecker" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#openModuleScreen" styleClass="module-button" text="%Checker"/>
        <Button alignment="CENTER_RIGHT" fx:id="buttonGenerator" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#openModuleScreen" styleClass="module-button" text="%Generator"/>
        <Separator maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"/>
        <Button alignment="CENTER_RIGHT" fx:id="buttonHowToUse" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#openModuleScreen" styleClass="module-button" text="%How.To.Use"/>
        <Button alignment="CENTER_RIGHT" fx:id="buttonAbout" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#openModuleScreen" styleClass="module-button" text="%About"/>
    </VBox>
    <StackPane HBox.hgrow="ALWAYS" fx:id="rightPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="right-pane"/>
</HBox>
